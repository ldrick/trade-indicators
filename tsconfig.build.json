{
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "dist",
		"target": "ESNext",
		"lib": ["ES2023"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"jsx": "react",
		"declaration": true,
		"pretty": true,
		"newLine": "lf",
		"stripInternal": true,
		"strict": true,
		"noEmitOnError": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules", "dist"],
	"include": ["src"]
}
